[{
	"type": "rpc",
	"id": "rpc.acme.setsettings",
	"params": {
		"type": "object",
		"properties": {
			"enable": {
				"type": "boolean",
				"required": true
			},
			"test_cert": {
				"type": "boolean",
				"required": true
			},
			"email": {
				"type": "string",
				"format": "email",
				"required": true
			},
			"keylength": {
				"type": "integer",
				"minimum": 2048,
				"maximum": 4096,
				"required": true
			},
			"env": {
				"type": "string",
				"required": false
			},
			"extraoptions": {
				"type": "string",
				"required": false
			},
			"certuuid": {
				"type": "string",
				"required": true
			}
		}
	}
},{
	"type": "rpc",
	"id": "rpc.acme.setdomain",
	"params": {
		"type": "object",
		"properties": {
			"uuid": {
				"type": "string",
				"format": "uuidv4",
				"required": true
			},
			"domain": {
				"type": "string",
				"required": true
			},
			"validation": {
				"type": "string",
				"enum": ["webroot", "dns_cf", "dns_he"],
				"required": true
			},
			"webroot": {
				"type": "string",
				"required": true
			}
		}
	}
},{
	"type": "rpc",
	"id": "rpc.acme.setenv",
	"params": {
		"type": "object",
		"properties": {
			"uuid": {
				"type": "string",
				"format": "uuidv4",
				"required": true
			},
			"key": {
				"type": "string",
				"required": true
			},
			"value": {
				"type": "string",
				"required": true
			}
		}
	}
}]
